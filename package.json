{
  "name": "sfbus-wb",
  "displayName": "SFBus WB",
  "version": "0.10.1",
  "description": "A starter kit for Angular applications using Tailwind CSS with dark mode support.",
  "homepage": "https://github.com/sfpayvn/SFBus-WB",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sfpayvn/SFBus-WB.git"
  },
  "keywords": [
    "angular",
    "tailwind",
    "starter-kit",
    "tailwind-template",
    "dark mode",
    "open source"
  ],
  "author": "Luciano Oliveira <me@lanno.dev>",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build --configuration development --output-path dist --base-href /sfbus-wb/browser/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:e2e": "npx playwright test --ui",
    "prettier": "prettier --config ./.prettierrc --write \"src/{app,environments}/**/*{.ts,.html,.scss,.json}\"",
    "prettier:verify": "prettier --config ./.prettierrc --check \"src/{app,environments}/**/*{.ts,.html,.scss,.json}\"",
    "prettier:staged": "pretty-quick --staged",
    "lint": "ng lint",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.4",
    "@angular/cdk": "^19.1.4",
    "@angular/common": "^19.1.4",
    "@angular/compiler": "^19.1.4",
    "@angular/core": "^19.1.4",
    "@angular/forms": "^19.1.4",
    "@angular/material": "^19.1.4",
    "@angular/platform-browser": "^19.1.4",
    "@angular/platform-browser-dynamic": "^19.1.4",
    "@angular/router": "^19.1.4",
    "@codemirror/lang-json": "^6.0.2",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.0.3",
    "@tailwindcss/typography": "^0.5.16",
    "angular-svg-icon": "^13.0.0",
    "apexcharts": "^3.35.3",
    "chart.js": "^4.5.1",
    "codemirror": "^6.0.2",
    "gh-pages": "^6.3.0",
    "grapesjs": "^0.22.14",
    "grapesjs-blocks-basic": "^1.0.2",
    "grapesjs-custom-code": "^1.0.2",
    "grapesjs-navbar": "^1.0.2",
    "grapesjs-plugin-forms": "^2.0.6",
    "grapesjs-preset-webpage": "^1.0.3",
    "grapesjs-tabs": "^1.0.6",
    "moment-timezone": "^0.5.47",
    "ng-apexcharts": "^1.7.1",
    "ng-zorro-antd": "^19.0.2",
    "ng2-charts": "^8.0.0",
    "ngx-cookie-service": "^19.1.0",
    "ngx-mask": "^19.0.6",
    "ngx-quill": "^24.0.0",
    "ngx-sonner": "^3.0.0",
    "quill": "^2.0.3",
    "rxjs": "~7.4.0",
    "socket.io-client": "^4.8.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.5",
    "@angular/cli": "^19.1.5",
    "@angular/compiler-cli": "^19.1.4",
    "@playwright/test": "^1.50.1",
    "@types/jasmine": "~3.10.0",
    "@types/lodash": "^4.17.15",
    "@types/node": "^12.11.1",
    "@types/socket.io-client": "^1.4.36",
    "autoprefixer": "^10.4.7",
    "cz-conventional-changelog": "^3.3.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.5.1",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "tailwind-scrollbar": "^4.0.0",
    "tailwindcss": "^4.0.5",
    "tailwindcss-animated": "^2.0.0",
    "typescript": "~5.7.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
